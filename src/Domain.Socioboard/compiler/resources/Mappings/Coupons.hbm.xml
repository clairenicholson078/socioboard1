<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="Domain.Socioboard.Models"
      assembly="Domain.Socioboard">
  <class name="Coupons" table="coupons" lazy="false">
    
      <id name="Id" column="Id" />
      <property name="CouponCode">
        <column name="CouponCode" />
      </property>
      <property name="EntryCouponDate">
        <column name="EntryCouponDate" />
      </property>
      <property name="ExpCouponDate">
        <column name="ExpCouponDate" />
      </property>
     
      <property name="Discount">
        <column name="Discount"  />
      </property>
  </class>
</hibernate-mapping>