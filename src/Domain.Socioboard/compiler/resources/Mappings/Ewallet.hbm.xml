<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="Domain.Socioboard.Models"
      assembly="Domain.Socioboard">
  <class name="Ewallet" table="Ewallet" lazy="false">
    <id name="Id" column="Id" />
    <property name="UserId">
      <column name="UserId" />
    </property>
    <property name="Transactiondate">
      <column name="Transactiondate"  />
    </property>
    <property name="EmailId">
      <column name="EmailId"  />
    </property>
    <property name="Amount">
      <column name="Amount"  />
    </property>
    <property name="Frommailid">
      <column name="Frommailid"  />
    </property>
    <property name="Fromid">
      <column name="Fromid"  />
    </property>
    <property name="TransactionId">
      <column name="TransactionId"  />
    </property>
    <property name="TransactionType">
      <column name="TransactionType"  />
    </property>
    <property name="TransactionStatus">
      <column name="TransactionStatus"  />
    </property>
  </class>
</hibernate-mapping>