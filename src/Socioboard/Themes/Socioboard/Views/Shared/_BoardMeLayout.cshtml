    <script>
        function myFunction() {
            $('#ImgLoading').show();
            $('#submitButton').hide();

            var firstName = $('#FirstName').val();
            var lastName = $('#LastName').val();
            var email = $('#EmailId').val();
            // var company = $('#req_Company').val();
            var Phone = $('#PhoneNumber').val();
            var password = $('#Password').val();
            var cnfPassword = $('#CnfPassword').val();
            var accountType = $('#AccountType').val()
            if (password != cnfPassword) {
                alert('password not match');
                $('#ImgLoading').hide();
                $('#submitButton').show();
                return false;
            }

            $.ajax({

                type: "POST",

                url: "@ViewBag.ApiDomain/api/user/Register",

                data: { "FirstName": firstName, "LastName": lastName, "EmailId": email, "PhoneNumber": Phone, "password": password, "AccountType": accountType },

                //contentType: "application/json; charset=utf-8",

                dataType: "html",

                success: function (msg) {
                    console.log(msg);
                    alertify.set({ delay: 5000 });
                    alertify.success("Email verification mail sent successfully.");
                    $('#FirstName').val("");
                    $('#LastName').val("");
                    $('#EmailId').val("");
                    $('#PhoneNumber').val("");
                    $('#Password').val("");
                    $('#CnfPassword').val("")
                    $('#ImgLoading').hide();
                    $('#submitButton').show();
                    // $('#req_skype').val("");
                },
                error: function (msg) {
                    alertify.set({ delay: 5000 });
                    alertify.error(msg.responseText);
                    $('#ImgLoading').hide();
                    $('#submitButton').show();
                    return false;
                }

            });
            $('#ImgLoading').hide();
            $('#submitButton').show();
            return false;
        }

        //function RegisterNow()
        //{
       //     $('#SignInModal').hide();
       //     $("#SignUpModal").modal();
        //}
    </script>



    <script>
        function Login() {
            $('#logLoading').show();
            $('input[type="submit"]').attr('disabled', true);
            $('#submitButton').hide();
            $("#fb-gp-login").css("display", "block");
            var userName = $('#logUserName').val();
            var password = $('#logPassword').val();


            $.ajax({

                type: "POST",

                url: "@ViewBag.Domain/Index/Login",

                data: { "UserName": userName, "Password": password },

                //contentType: "application/json; charset=utf-8",

                dataType: "html",

                success: function (msg) {
                    $("#fb-gp-login").css("display", "none");
                    if (msg == "UpgradePlans") {
                        alertify.alert("your account has been expired.Please upgrade it.");
                        setTimeout(function () {
                            window.top.location = '/Index/UpgradePlans'
                        }, 3000);
                    }
                    if (msg == "1") {
                        $('input[type="submit"]').attr('disabled', false);
                        if ($("#RememberMe").is(':checked')) {

                            checkCookie(userName, password);

                        }
