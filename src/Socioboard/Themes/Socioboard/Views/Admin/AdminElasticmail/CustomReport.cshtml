                                <script>
                                    
                                    function enable_btn() {
                                        $("#submit_date").attr("class", "waves-effect waves-light btn cyan center");
                                        document.getElementById("submit_date").disabled = false;
                                    };
                                    function customreport() {
                                        debugger;
                                        var startdate = $('#dates').val();
                                        var enddate = $('#dates1').val();
                                        if (startdate != "" && enddate != "") {
                                            //date should be converted here
                                            $("#submit_date").attr("class", "waves-effect waves-light btn grey center");
                                            document.getElementById("submit_date").disabled = true;
                                            GetCustomSentMailReport(startdate, enddate);
                                            GetCustomOpenedMailReport(startdate, enddate);
                                            GetCustomClickedMailReport(startdate, enddate);
                                            GetCustomBouncedMailReport(startdate, enddate);
                                            GetCustomUnsubscribedMailReport(startdate, enddate);
                                            GetCustomAbuseReportedMailReport(startdate, enddate);
                                        }
                                        else
                                        {
                                            swal("Date should not be empty!");
                                        }
                                    };
                                    

                                    function GetCustomSentMailReport(startdate, enddate) {
                                        
                                        $.ajax({

                                            type: "GET",

                                            url: '../AdminElasticmail/GetCustomSentMailReport?startdate=' + startdate +'&enddate='+enddate,
                                            dataType: "html",
                                            success: function (msg) {
                                            
                                                    var msg_conv = msg > 999999 ? (msg / 1000000).toFixed(1) + ' M' : (msg > 999 ? (msg / 1000).toFixed(1) + ' K' : msg);
                                                    $("#GetSentMailReport").html(msg_conv);
                                                

                                            }

                                        });

                                    }
                                    function GetCustomOpenedMailReport(startdate, enddate) {

                                        $.ajax({

                                            type: "GET",

                                            url: '../AdminElasticmail/GetCustomOpenedMailReport?startdate=' + startdate + '&enddate=' + enddate,
                                            dataType: "html",
                                            success: function (msg) {
                                              
                                                    var msg_conv = msg > 999999 ? (msg / 1000000).toFixed(1) + ' M' : (msg > 999 ? (msg / 1000).toFixed(1) + ' K' : msg);
                                                    $("#GetOpenedMailReport").html(msg_conv);
                                               
                                               

                                            }

                                        });

                                    }
                                    function GetCustomClickedMailReport(startdate, enddate) {

                                        $.ajax({

                                            type: "GET",

                                            url: '../AdminElasticmail/GetCustomClickedMailReport?startdate=' + startdate + '&enddate=' + enddate,
