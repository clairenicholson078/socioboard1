<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="Domain.Socioboard.Models"
      assembly="Domain.Socioboard">

  <class name="YoutubeGroupInvite" table="youtubegroups" lazy="false">
    <id name="Id" column="Id" />
    <property name="UserId">
      <column name="UserId" />
    </property>
    <property name="Owner">
      <column name="Owner" />
    </property>
    <property name="OwnerName">
      <column name="OwnerName" />
    </property>
    <property name="OwnerEmailid">
      <column name="OwnerEmailid" />
    </property>
    <property name="Active">
      <column name="Active" />
    </property>
    <property name="SBUserName">
      <column name="SBUserName" />
    </property>
    <property name="SBEmailId">
      <column name="SBEmailId" />
    </property>
    <property name="SBProfilePic">
      <column name="SBProfilePic" />
    </property>
    <property name="AccessSBUserId">
      <column name="AccessSBUserId" />
    </property>
    <property name="EmailValidationToken">
      <column name="EmailValidationToken" />
    </property>
  </class>
</hibernate-mapping>