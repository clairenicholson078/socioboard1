<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="Domain.Socioboard.Models"
      assembly="Domain.Socioboard">
  <class name="MongoBoards" table="MongoBoards" lazy="false">
    <id name="id" column="id" />
    <property name="boardName">
      <column name="boardName" />
    </property>
    <property name="TempboardName">
      <column name="TempboardName" />
    </property>
    <property name="boardId">
      <column name="boardId" />
    </property>
    <property name="userId">
      <column name="userId"/>
    </property>
    <property name="trendingtype">
      <column name="trendingtype"/>
    </property>
    <property name="createDate">
      <column name="createDate"/>
    </property>
    <property name="isActive">
      <column name="isActive"/>
    </property>
    <property name="facebookHashTag">
      <column name="facebookHashTag"/>
    </property>
    <property name="twitterHashTag">
      <column name="twitterHashTag"/>
    </property>
    <property name="instagramHashTag">
      <column name="instagramHashTag"/>
    </property>
    <property name="gplusHashTag">
      <column name="gplusHashTag"/>
    </property>
  </class>
</hibernate-mapping>