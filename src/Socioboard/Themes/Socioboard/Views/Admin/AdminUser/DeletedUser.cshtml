    <script type="text/javascript">
        $('#manage_user_table').DataTable();
        $('.modal-trigger').leanModal();
        $('select').material_select();

        undoProfile = function(Id){
            // console.log(profileId);
            swal({
            title: "Are you sure?",
            text: "This profile will be activated",
            type: "warning",
            showCancelButton: true,
            confirmButtonColor: "#51d168",
            confirmButtonText: "Yes, Active it!",
            closeOnConfirm: false },
            function () {
                debugger;
                //todo: code to active profile

                $.ajax({

                    type: "GET",

                    url: '../AdminUser/UndoUser?Id=' + Id,
                    dataType: "html",

                    success: function (msg) {

                        swal("Activated!", "Profile has been activated.", "success");

                    }

                });

                setTimeout(function () {
                    swal("Activated!", "Profile has been activated.", "success");
                    window.location.reload();
                }, 3000);

                });
        }
    </script>

