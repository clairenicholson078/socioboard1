<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="Domain.Socioboard.Models"
      assembly="Domain.Socioboard">
  <class name="Affiliates" table="Affiliates" lazy="false">
    <id name="Id" column="Id" />
    <property name="UserId">
      <column name="UserId" />
    </property>
    <property name="FriendUserId">
      <column name="FriendUserId"  />
    </property>
    <property name="AffiliateDate">
      <column name="AffiliateDate"  />
    </property>
    <property name="Amount">
      <column name="Amount"  />
    </property>
  </class>
</hibernate-mapping>