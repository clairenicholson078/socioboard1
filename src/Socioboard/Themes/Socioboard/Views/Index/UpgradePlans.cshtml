                        <script>
        function myFunction() {
            debugger;
            $('#ImgLoading').show();
            $('#submitButton').hide();

            var firstName = $('#FirstName').val();
            var lastName = $('#LastName').val();
            var email = $('#EmailId').val();
            // var company = $('#req_Company').val();
            var Phone = $('#PhoneNumber').val();
            var password = $('#Password').val();
            var cnfPassword = $('#CnfPassword').val();
            var accountType = $('#AccountType').val();
            var PaymentType = $('#PaymentType').val();
            if (password != cnfPassword) {
                alert('password not match');
                $('#ImgLoading').hide();
                $('#submitButton').show();
                return false;
            }

            $.ajax({

                type: "POST",

                url: "@ViewBag.ApiDomain/api/user/Register",

                data: { "FirstName": firstName, "LastName": lastName, "EmailId": email, "PhoneNumber": Phone, "password": password, "AccountType": accountType, "PaymentType": PaymentType },

                //contentType: "application/json; charset=utf-8",

                dataType: "html",

                success: function (msg) {
                    console.log(msg);
                    alertify.set({ delay: 5000 });
                    alertify.success("Email verification mail sent successfully.");
                    $('#FirstName').val("");
                    $('#LastName').val("");
                    $('#EmailId').val("");
                    $('#PhoneNumber').val("");
                    $('#Password').val("");
                    $('#CnfPassword').val("")
                    $('#ImgLoading').hide();
                    $('#submitButton').show();
                    // $('#req_skype').val("");
                    if (accountType != "0") {
                        $.ajax({

                            type: "GET",

                            url: "@ViewBag.Domain/Home/PayPalAccount",

                            data: { "emailId": email, "IsLogin": false },

                            dataType: "html",
                            success: function (msg) {
                                alertify.set({ delay: 5000 });
                                alertify.error("please submit Payment detail.");
                                window.top.location = msg;
                            },
                            error: function (msg) {

                                return false;
                            }
                        });
                    }
                },
                error: function (msg) {
                    alertify.set({ delay: 5000 });
                    alertify.error(msg.responseText);
                    $('#ImgLoading').hide();
                    $('#submitButton').show();
                    return false;
                }

            });
            $('#ImgLoading').hide();
            $('#submitButton').show();
            return false;
        }


