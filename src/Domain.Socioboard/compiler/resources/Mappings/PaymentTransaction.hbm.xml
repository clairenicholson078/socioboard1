<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="Domain.Socioboard.Models"
      assembly="Domain.Socioboard">
  <class name="PaymentTransaction" table="paymenttransaction" lazy="false">
    <id name="id" column="id" />
    <property name="userid">
      <column name="userid" />
    </property>
    <property name="email">
      <column name="email"  />
    </property>
    <property name="amount">
      <column name="amount"  />
    </property>
    <property name="paymentdate">
      <column name="paymentdate"  />
    </property>
    <property name="PaymentType">
      <column name="PaymentType"  />
    </property>
    <property name="paymentId">
      <column name="paymentId"  />
    </property>
    <property name="trasactionId">
      <column name="trasactionId"  />
    </property>
    <property name="media">
      <column name="media"  />
    </property>
    <property name="itemname">
      <column name="itemname"  />
    </property>
    <property name="paymentstatus">
      <column name="paymentstatus"  />
    </property>
    <property name="Payername">
      <column name="Payername"  />
    </property>
    <property name="payeremail">
      <column name="payeremail"  />
    </property>
    <property name="subscrdate">
      <column name="subscrdate"  />
    </property>
  <property name="bluesnapSubscriptions">
      <column name="bluesnapSubscriptions"  />
    </property>
  </class>
</hibernate-mapping>