<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
      xmlns="urn:nhibernate-mapping-2.2"
      namespace="Domain.Socioboard.Models"
      assembly="Domain.Socioboard">
  <class name="FeedBack" table="FeedBack" lazy="false">
    <id name="Id" column="Id" />
    <property name="UserId">
      <column name="UserId" />
    </property>
    <property name="createddate">
      <column name="createddate"  />
    </property>
    <property name="EmailId">
      <column name="EmailId"  />
    </property>
    <property name="UserActivationStatus">
      <column name="UserActivationStatus"  />
    </property>
    <property name="AccountType">
      <column name="AccountType"  />
    </property>
    <property name="RegisterAccountType">
      <column name="RegisterAccountType"  />
    </property>
    <property name="FeedBackMsg">
      <column name="FeedBackMsg"  />
    </property>
  </class>
</hibernate-mapping>